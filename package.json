    "scripts": {
        "analyze": "source-map-explorer build/static/js/main.*",
        "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --usePolling --polling-interval 500",
        "start-js": "cross-env PORT=3002 react-app-rewired start",
        "start-dev": "copy \"./.env.dev\" \"./.env\" && npm-run-all -p watch-css start-js",
        "start-qa": "copy \"./.env.qa\" \"./.env\" && npm-run-all -p watch-css start-js",
        "start-staging": "copy \"./.env.staging\" \"./.env\" && npm-run-all -p watch-css start-js",
        "build-js": "react-app-rewired build --max_old_space_size=4096",
        "build": "copy \"./.env.prod\" \"./.env\" && npm-run-all build-css build-js",
        "build-prod": "copy \"./.env.prod\" \"./.env\" && npm-run-all build-css build-js",
        "build-dev": "copy \"./.env.dev\" \"./.env\" && npm-run-all build-css build-js",
        "build-qa": "copy \"./.env.qa\" \"./.env\" && npm-run-all build-css build-js",
        "build-staging": "copy \"./.env.staging\" \"./.env\" && npm-run-all build-css build-js",
        "format": "prettier \"src/**/*.+(js|jsx|ts|tsx|json|css|scss|md)\" --write",
        "precommit": "lint-staged",
        "test": "react-app-rewired test",
        "test:coverage": "react-app-rewired test --coverage",
        "eject": "react-app-rewired eject"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": "react-app",
        "plugins": [
            "react-hooks"
        ],
        "rules": {
            "react-hooks/rules-of-hooks": "error",
            "react-hooks/exhaustive-deps": "warn"
        }
    },
    "devDependencies": {
        "@types/jest": "^24.0.11",
        "cross-env": "^6.0.0",
        "customize-cra": "^0.2.12",
        "eslint-plugin-react-hooks": "^1.7.0",
        "jest-dom": "^3.1.3",
        "lint-staged": "^8.1.5",
        "prettier": "^1.16.4",
        "react-app-rewired": "^2.1.1",
        "react-testing-library": "^6.1.2",
        "source-map-explorer": "^2.0.1"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
